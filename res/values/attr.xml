<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="IntegerListPreference">
        <attr name="entryList" format="reference" />
        <attr name="valueList" format="reference" />
    </declare-styleable>

    <declare-styleable name="PreferenceKeys">
        <attr name="keysToReset" format="reference" />
    </declare-styleable>

    <declare-styleable name="SliderPreference">
        <attr name="description" format="string|reference" />
        <attr name="unitType" format="reference">
            <flag name="temperature" value="0" />
            <flag name="windspeed" value="1" />
        </attr>
        <attr name="defaultUnit" format="integer|reference" />
        <attr name="minimumValue" format="integer|reference" />
        <attr name="maximumValue" format="integer|reference" />
        <attr name="contentDescription" format="string|reference" />
        <attr name="src" format="integer|reference" />
    </declare-styleable>

</resources>